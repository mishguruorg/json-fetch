{
  "name": "@mishguru/json-fetch",
  "version": "8.1.0",
  "description": "A wrapper around ES6 fetch to simplify interacting with JSON APIs.",
  "author": "George Czabania <george@mish.guru>",
  "contributors": [
    "stayradiated <dev@stayradiated.com>",
    "dannynelson <danny@goodeggs.com>"
  ],
  "license": "MIT",
  "keywords": [
    "fetch",
    "api",
    "json",
    "es6"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/mishguru/json-fetch.git"
  },
  "homepage": "https://github.com/mishguru/json-fetch",
  "bugs": "https://github.com/mishguru/json-fetch/issues",
  "scripts": {
    "build": "pkg-build",
    "coverage": "NODE_ENV=test_local pkg-coverage",
    "flow": "pkg-flow",
    "lint": "pkg-lint",
    "prepublish": "pkg-build",
    "test": "NODE_ENV=test_local pkg-test",
    "test-only": "NODE_ENV=test_local pkg-test-only"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "lodash.pick": "^4.4.0",
    "node-fetch": "^2.0.0-alpha.9",
    "promise-retry": "^1.1.1"
  },
  "devDependencies": {
    "@mishguru/package": "^1.15.4",
    "nock": "^9.1.6",
    "sinon": "^4.2.0",
    "stu": "^1.3.0"
  },
  "babel": {
    "presets": [
      "flow",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
